/*--------------------start--------------------------*/

	.full-width
	{
		width:100%;
	}
	
	.header
	{
		width:100%;
		
	}
   .head{
        width: 100%;
        height: 100px;
		position:fixed;
		top:0;
		left:0;
		background:#fff;
    }

    .logo{
        width: 20%;
        height: 100px;
        margin-left: 100px;
        float: left;
    }
    .logo>img{
        width: 100%;
        height: 100px;
    }

    .menu{
        width: 55%;
        height: 100px;
        float: right;
    }

    .main{

        list-style: none;
        padding: 23px;
		padding-right:30px;
        
    }
    .main>li{
        display: inline;
        padding: -25px;
		padding-right:30px;
        /* margin-top: 20px; */
    }
    .main>li>a{
        text-decoration: none;
        color: black;
        font-weight: bold;
		font-size:22px;
    }
    .icon>a>i{
        padding-left: 11px;
        font-size: 22px;
    }
	
	.image
	{
		width:100%;
		height:700px;
	}
	
	.image{
	 background-image:url('../photos/bg1.webp');
	/* background-position:0 -200px; */
	background-size:100%;
	background-position:center;
    background-attachment:fixed;
	background-repeat:none;
	 position: absolute;
	}
	.image h3
	{
	 /* padding:100px 424px; */
     color:white;	
padding:120px;	 
	 text-align:center;
	}
	
	.image p
	{
		text-align:center;
		color:white;
		font-size:90px;
	}
	
	.our
	{
		width:100%;
		height:400px;
		background:#f00;
	}
	
	.time
	{
		width:100%;
		height:600px;
		background:#666;
	}
	
	.food
	{
		width:100%;
		height:500px;
		background:#f00;
	}
	
	.last
	{
		width:100%;
		height:400px;
		background:#ddd;
	}
	
	
	
